{
  "version": 2,
  "builds": [
    {
      "src": "dist/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(dist/[^/]+\\.js)",
      "dest": "dist/:1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(dist/[^/]+\\.css)",
      "dest": "dist/:1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(dist/[^/]+\\.[a-f0-9]+\\.chunk\\.js)",
      "dest": "dist/:1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(dist/[^/]+\\.[a-f0-9]+\\.chunk\\.css)",
      "dest": "dist/:1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ]
}
