{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node",
      "env": {
        "FFMPEG_PATH": "/vercel/ffmpeg",
        "FFPROBE_PATH": "/vercel/ffprobe"
      },
      "commands": ["ffmpeg-static -d ffmpeg", "ffprobe-static -d ffprobe"]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
